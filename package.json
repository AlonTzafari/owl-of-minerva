  },
  "homepage": "https://github.com/AlonTzafari/owl-of-minerva#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.9",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.13",
    "node-cron": "^3.0.0",
    "socks-proxy-agent": "^5.0.0"
  },
  "devDependencies": {
